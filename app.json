{
  "expo": {
    "name": "CC Tool",
    "slug": "cc-tool",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "version": "1.0.5",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.allisonf.cctool",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "This app needs to access your local network to connect to your 3D printers.",
        "NSBonjourServices": ["_bonjour._tcp", "_lnp._tcp."]
      }    
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.allisonf.cctool",
      "versionCode": 7
    },
    "developmentClient": {
      "silenceDeploymentWarnings": true
    }
  }
}
